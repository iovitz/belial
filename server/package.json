    "eslint": "9.26.0",
    "hot-hook": "0.4.0",
    "pino-pretty": "13.0.0",
    "prettier": "3.5.0",
    "ts-node-maintained": "10.9.5",
    "typescript": "5.8.3"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "argon2"
    ]
  }
